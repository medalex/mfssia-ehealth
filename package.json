{
  "name": "dkg-client",
  "version": "0.0.1",
  "description": "",
  "author": "Sunday Aroh",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "env-cmd typeorm-ts-node-commonjs -d src/database/data-source.ts",
    "migration:up": "yarn typeorm migration:run",
    "migration:down": "yarn typeorm migration:revert",
    "migration:show": "yarn typeorm migration:show",
    "migration:create": "typeorm migration:create",
    "migration:generate": "yarn typeorm migration:generate --pretty",
    "db:create": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs db:create",
    "db:drop": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs db:drop",
    "seed:run": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run",
    "seed:create": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "size": "hardhat size-contracts",
    "clean": "hardhat clean",
    "serve": "npx hardhat node",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile --show-stack-traces",
    "deploy": "hardhat run --network localhost scripts/deploy.ts",
    "deploy-sepolia": "hardhat run --network sepolia contracts/scripts/deploy.ts",
    "accounts": "hardhat accounts"
  },
  "dependencies": {
    "@chainlink/contracts": "^1.5.0",
    "@nestjs/common": "9.1.6",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.6",
    "@nestjs/platform-express": "9.1.6",
    "@nestjs/swagger": "6.1.3",
    "@nestjs/typeorm": "9.0.1",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dkg.js": "8.2.2",
    "es6-shim": "^0.35.8",
    "ethers": "^6.16.0",
    "helmet": "^6.0.1",
    "multer": "1.4.4",
    "multer-s3": "2.10.0",
    "nestjs-i18n": "9.2.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "4.5.0",
    "typeorm": "0.3.10",
    "typeorm-extension": "^3.7.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.6",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/multer": "1.4.7",
    "@types/node": "16.18.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "aws-sdk": "2.1243.0",
    "env-cmd": "10.1.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "hardhat": "^2.22.0",
    "hardhat-gas-reporter": "^2.3.0",
    "is-ci": "3.0.1",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "solidity-coverage": "^0.8.17",
    "supertest": "6.3.1",
    "ts-jest": "29.0.1",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "tslib": "2.4.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
