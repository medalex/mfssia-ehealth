    "seed:create": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "size": "hardhat size-contracts",
    "clean": "hardhat clean",
    "serve": "npx hardhat node",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile --show-stack-traces",
    "deploy": "hardhat run --network localhost scripts/deploy.ts",
    "deploy-sepolia": "hardhat run --network sepolia contracts/scripts/deploy.ts",
    "deploy-upgrade": "hardhat run --network sepolia contracts/scripts/upgrade.ts",
    "deploy-test": "hardhat run --network sepolia contracts/scripts/test-oracle-request.ts",
    "accounts": "hardhat accounts"
  },
